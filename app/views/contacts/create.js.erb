<% if @contact.save %>
  $("#j_contact").show("<%= j(render partial: 'contacts/complete') %>");
  $("#ajax_contact_table").hide();
  $("#ji_contact").html("<%= j(render partial: 'contacts/contact') %>");
<% else %>
  $("#new_msg").html('<div style="color:red; margin-bottom:10px;"><%= @msgs %></div>');
<% end %>
