<% if @claim.save %>
  $(".claim_form").hide();
  $("#j_claim").show("<%= j(render partial: 'contacts/complete') %>");
  $("#ji_claim").html("<%= j(render partial: 'claims/claim') %>");
  $('.claim_modal_wrapper').hide('slow');
<% else %>
  $("#new_msg").html('<div style="color:red; margin-bottom:10px;"><%= @msgs %></div>');
<% end %>